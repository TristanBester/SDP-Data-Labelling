{
  "name": "SDP-Data-Labelling",
  "version": "1.0.0",
  "description": "Someone update this thing.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint **/*.js --fix",
    "lint-fix": "prettier-eslint **/*.js --write",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint-fix",
      "git add"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faisal2754/SDP-Data-Labelling.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/faisal2754/SDP-Data-Labelling/issues"
  },
  "homepage": "https://github.com/faisal2754/SDP-Data-Labelling#readme",
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1"
  }
}
